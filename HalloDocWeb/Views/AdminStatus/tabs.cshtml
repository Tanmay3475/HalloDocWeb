<script>
    $(document).ready(function () {

        $('.dashboardtab').on('click', function (e) {
            e.preventDefault();
            $('.dashboardtab').removeClass('active');
            $(this).addClass('active');
            var target = $(this).data('bs-target');
            localStorage.setItem("target", target);
            console.log;
            var url;
            switch  {
                case '#status_new':
                $('#statusnamehead').html('New');
                url = '/Admin/New';

                break;
                case '#status_pending':
                $('#statusnamehead').html('Pending');

                url = '/Admin/Pending';
                break;
                case '#status_active':
                $('#statusnamehead').html('Active');

                url = '/Admin/Active';
                break;
                case '#status_conclude':
                $('#statusnamehead').html('Conclude');

                url = '/Admin/Conclude';
                break;
                case '#status_toclose':
                $('#statusnamehead').html('To Close');

                url = '/Admin/Toclose';
                break;
                case '#status_unpaid':
                $('#statusnamehead').html('Unpaide');

                url = '/Admin/Unpaid';
                break;

                default:
                    url = '../Home/AdminLogin';
            }
              console.log(url);
            $.ajax({
                url: url,

                success: function (response) {
                    $('#datatable').html(response);

                },
                error: function (xhr, status, error) {
                    console.error(error);
                }
            });
        });
    });

</script>