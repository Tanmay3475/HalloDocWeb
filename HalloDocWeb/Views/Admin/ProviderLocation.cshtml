<body>
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom" style="width: 80rem;left:300px;position:absolute;">
        <h2 class="mt-2 float-start "> Provider Location</h2>
        <button class="btn btn-outline-info float-end mt-2 me-5 back-button">
            < Back
        </button>


    </div>
<div id="map"></div>

</body>
<script src="/js/site.js"></script>
<script>
    var map = L.map('map').setView([51.505, -0.09], 13); 
    L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
        maxZoom: 19,
        attribution: '&copy; <a href="http://www.openstreetmap.org/copyright">OpenStreetMap</a>'
    }).addTo(map);
    var marker = L.marker([51.5, -0.09]).addTo(map);
    var marker = L.marker([55.5, -0.1]).addTo(map);
    var marker = L.marker([53.5, 0.09]).addTo(map);
    var marker = L.marker([56.5, -2.09]).addTo(map);
    var marker = L.marker([61.5, -0.09]).addTo(map);
    var marker = L.marker([71.5, -0.09]).addTo(map);
    var marker = L.marker([81.5, -0.09]).addTo(map);

    var popup = L.popup();

    function onMapClick(e) {
        popup
            .setLatLng(e.latlng)
            .setContent("You clicked the map at " + e.latlng.toString())
            .openOn(map);
    }

    map.on('click', onMapClick);</script>
    
